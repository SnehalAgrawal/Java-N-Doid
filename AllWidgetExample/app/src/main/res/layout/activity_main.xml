            android:id="@+id/bwebview"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:onClick="clicktogo"
            android:text="WebView" />

        <Button
            android:id="@+id/bsearchview"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:onClick="clicktogo"
            android:text="SearchView" />

