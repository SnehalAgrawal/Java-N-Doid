<?xml version="1.0" encoding="utf-8"?>
<ScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        android:padding="16dp">

        <Button
            android:id="@+id/btoast"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:onClick="clicktogo"
            android:text="Button(Toast)" />

        <Button
            android:id="@+id/bedittext"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:onClick="clicktogo"
            android:text="EditText/TextView Example" />

        <ImageButton
            android:id="@+id/bimagebutton"
            android:layout_width="match_parent"
            android:layout_height="50dp"
            android:onClick="clicktogo"
            android:src="@mipmap/ic_launcher_round" />

        <Button
            android:id="@+id/bcheckbox"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:onClick="clicktogo"
            android:text="Checkbox" />

        <Button
            android:id="@+id/bradiobutton"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:onClick="clicktogo"
            android:text="Radio Button" />

        <Button
            android:id="@+id/bspinner"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:onClick="clicktogo"
            android:text="Spinner/Dropdown" />

        <Button
            android:id="@+id/bautocomplete"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:onClick="clicktogo"
            android:text="Auto Complete Text" />

        <Button
            android:id="@+id/blistview"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:onClick="clicktogo"
            android:text="ListView" />

        <Button
            android:id="@+id/bgridview"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:onClick="clicktogo"
            android:text="GridView" />

        <Button
            android:id="@+id/btabhost"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:onClick="clicktogo"
            android:text="TabHost" />

        <Button
            android:id="@+id/bquickcontactbatch"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:onClick="clicktogo"
            android:text="Quick contactc batch" />

        <Button
            android:id="@+id/bmislenious"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:onClick="clicktogo"
