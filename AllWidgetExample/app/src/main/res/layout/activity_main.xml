            android:id="@+id/bdatepicker"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:onClick="clicktogo"
            android:text="DatePicker" />

        <Button
            android:id="@+id/bcalenderview"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:onClick="clicktogo"
            android:text="CalenderView" />

        <Button
            android:id="@+id/bchronometer"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:onClick="clicktogo"
            android:text="Chronometer" />

        <Button
            android:id="@+id/btextclock"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:onClick="clicktogo"
            android:text="Text Clock" />


        <Button
            android:id="@+id/bimageswitcher"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:onClick="clicktogo"
            android:text="Image Switcher" />

        <Button
            android:id="@+id/badapterviewflipper"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:onClick="clicktogo"
            android:text="Adapter View Flipper" />

        <Button
            android:id="@+id/bviewflipper"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:onClick="clicktogo"
            android:text="View Flipper" />

        <Button
            android:id="@+id/bstackview"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:onClick="clicktogo"
            android:text="Stack View" />

        <Button
            android:id="@+id/btextswitcher"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:onClick="clicktogo"
            android:text="Text Switcher" />

        <Button
            android:id="@+id/bviewanimator"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:onClick="clicktogo"
            android:text="View Animator" />

        <Button
            android:id="@+id/bviewswitcher"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:onClick="clicktogo"
            android:text="View Switcher" />

        <Button
            android:id="@+id/bviewstub"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:onClick="clicktogo"
            android:text="View Stub" />

        <Button
            android:id="@+id/btaxtureview"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:onClick="clicktogo"
            android:text="Taxture View" />

        <Button
            android:id="@+id/bsurfaceview"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:onClick="clicktogo"
            android:text="Surface View" />

        <Button
            android:id="@+id/brecycler"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:onClick="clicktogo"
